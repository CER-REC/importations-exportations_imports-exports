{
  "name": "import-export-visualization",
  "version": "1.1.0",
  "description": "visualization of import export data for the NEB",
  "main": "index.js",
  "scripts": {
    "build:data": "babel-node ./data.js",
    "build:portmap:shp2json": "shp2json -n ./srcData/gpr_000b11a_e.shp -o ./srcData/shp2json.json",
    "build:portmap:geo2topo": "geo2topo -n regions=./srcData/shp2json.json -o ./srcData/geo2topo.json",
    "build:portmap:toposimplify": "toposimplify -n -S 1e-3 -F ./srcData/geo2topo.json -o ./srcData/toposimplify.json",
    "build:portmap:topoquantize": "topoquantize -n -o ./srcData/topoquantize.json 1e3 ./srcData/toposimplify.json",
    "build:portmap": "npm run build:portmap:shp2json && npm run build:portmap:geo2topo && npm run build:portmap:toposimplify && npm run build:portmap:topoquantize && cp ./srcData/topoquantize.json ./public/data/topo.json",
    "test": "mocha-webpack --watch --recursive ./test/",
    "webpack": "webpack --config webpack.prod.config.js",
    "start": "babel-node servers/DevelopmentServer/DevelopmentServer.js",
    "lint": "eslint --ext .jsx,.js app/**",
    "build": "node ./build.js"
  },
  "author": "Patrick King, Rahul Bhaskar, Charlie Cheung",
  "license": "Open Government Liscense - Canada",
  "dependencies": {
    "ajv": "^5.2.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bluebird": "^3.5.0",
    "browser-cookies": "^1.1.0",
    "client-request": "^2.3.0",
    "css-loader": "^0.28.4",
    "d3-geo": "^1.9.1",
    "dialog-polyfill": "^0.4.9",
    "domready": "^1.0.8",
    "dot-env": "0.0.1",
    "dotenv": "^4.0.0",
    "dotenv-webpack": "^1.5.4",
    "express": "^4.15.3",
    "history": "^4.7.2",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "lzutf8": "^0.4.7",
    "memoize-immutable": "^3.0.0",
    "mustache": "^2.3.0",
    "mustache-express": "^1.2.5",
    "node-sass": "^4.5.3",
    "nsp": "^2.7.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-hot-loader": "^4.0.0-beta.22",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "reselect": "^3.0.1",
    "sass-loader": "^6.0.6",
    "should": "^11.2.1",
    "style-loader": "^0.18.2",
    "topojson-client": "^3.0.0",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
